<?xml version="1.0" encoding="utf-8"?>
<permissions>
    <privapp-permissions package="com.lenovo.charactercallsetting">
        <permission name="android.permission.MODIFY_PHONE_STATE" />
        <permission name="android.permission.READ_PRIVILEGED_PHONE_STATE" />
        <!-- "WRITE_SECURE_SETTINGS" permission is default allowed for system process,need to apply it here to
           avoid secure exception after "android.uid.system" removed -->
        <permission name="android.permission.WRITE_SECURE_SETTINGS" />
    </privapp-permissions>
</permissions>
